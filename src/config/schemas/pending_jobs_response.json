{
    "$comment": "pending jobs response",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "string",
            "pattern": "^mosip.abis.pendingJobs$"
        },
        "requestId": {
            "type": "string"
        },
        "responsetime" : {
            "type": "string"
        },
        "jobscount" : {
            "type": "string",
            "pattern":  "^\\d*$"
        },
        "returnValue" : {
            "type": "integer",
            "minimum":  0,
            "maximum": 9
        },
        "failureReason": {
            "type": "integer",
            "minimum": 0,
            "maximum": 9
        }
    },
    "required": ["id", "requestId", "responsetime", "jobscount", "returnValue"]
}